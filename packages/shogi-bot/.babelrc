{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "11.11.0"
        },
        "modules": "commonjs"
      }
    ]
  ],
  "plugins": [
    "add-module-exports",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-modules-commonjs",
    "@babel/plugin-transform-runtime",
    [
      "babel-plugin-inline-import", {
        "extensions": [
          ".dot",
          ".md",
          ".proto"
        ]
      }
    ]
  ]
}